{% set logoImgPath="/image/svg/navbar/logo-dark.svg"%}
{% set item1="Industries"%}
{% set item2="Product"%}
{% set item3="Pricing"%}
{% set item4="About"%}
{% set buttonName1="Button"%}
{% set buttonName2="Button"%}

<!-- hero 12 dark version -->
<div class="hero_navigation-light">
    <div class="hero_navigation-logo-light">
        <img src="{{logoImgPath}}" alt="">
    </div>
    <div class="hero_navigation-menu-light">
        <ul class="hero-navigation-list-light">
            <li class="hero-list-item-light">{{item1}}</li>
            <li class="hero-list-item-light">{{item2}}</li>
            <li class="hero-list-item-light">{{item3}}</li>
            <li class="hero-list-item-light">{{item4}}</li>
        </ul>
    </div>
    <div class="hero_navigation-buttons-light">
        <button class="btn btn-light-h12Light">{{buttonName1}}</button>
        <button class="btn btn-dark-h12Light">{{buttonName2}}</button>
    </div>
</div>