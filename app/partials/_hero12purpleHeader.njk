{% set logoImgPath="/image/svg/navbar/light-logo.svg"%}
{% set item1="Industries"%}
{% set item2="Product"%}
{% set item3="Pricing"%}
{% set item4="About"%}
{% set buttonName1="Button"%}
{% set buttonName2="Button"%}

<!-- hero 12 dark version -->
<div class="hero_navigation-purple">
    <div class="hero_navigation-logo-light">
        <img src="{{logoImgPath}}" alt="">
    </div>
    <div class="hero_navigation-menu-purple">
        <ul class="hero_navigation-list-purple">
            <li class="hero-list-item-purple">{{item1}}</li>
            <li class="hero-list-item-purple">{{item2}}</li>
            <li class="hero-list-item-purple">{{item3}}</li>
            <li class="hero-list-item-purple">{{item4}}</li>
        </ul>
    </div>
    <div class="hero_navigation-buttons-purple">
        <button class="btn btn-light-h12Purple">{{buttonName1}}</button>
        <button class="btn btn-dark-h12Purple">{{buttonName2}}</button>
    </div>
</div>